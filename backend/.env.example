# Google Cloud Vertex AI 配置
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_CLOUD_LOCATION=us-central1
GOOGLE_APPLICATION_CREDENTIALS=../your-credentials-file.json
PORT=8080
GIN_MODE=debug

# 数据库配置
DB_TYPE=postgres  # 可选值: postgres, mysql
DB_HOST=localhost
DB_PORT=5432  # PostgreSQL默认端口，MySQL默认为3306
DB_USER=postgres  # PostgreSQL默认用户，MySQL默认用户为root
DB_PASSWORD=your-password
DB_NAME=ai_resume
DB_SSLMODE=disable  # 仅PostgreSQL使用
DB_CHARSET=utf8mb4  # 仅MySQL使用

# JWT配置
JWT_SECRET_KEY=your-secret-key-change-in-production

# Google OAuth配置
GOOGLE_CLIENT_ID=your-google-client-id

# Cloudflare Turnstile配置
TURNSTILE_SECRET_KEY=your-turnstile-secret-key
